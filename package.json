{
  "name": "newproject",
  "version": "1.0.0",
  "description": "Proyecto de node.js",
  "main": "index.html",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "lite-server",
    "start": "concurrently \"npm run watch:scss\" \"npm run dev \"",
    "sass-dev": "sass --watch --update --style=expanded assets/scss:assets/css",
    "sass-prod": "sass --no-source-map --style=compressed assets/scss:assets/css",
    "scss": "sass --watch --update ../:../",
    "watch:scss": "onchange './*.scss' -- npm run scss",
    "clean": "rimraf dist",
    "imagemin": "imagemin ../newproject/* --out-dir ../newproject/dist",
    "usemin": "usemin ../index.html -d dist --htmlmin -o dist/index.html",
    "build": "npm run clean && npm run scss && npm run imagemin && npm run usemin"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Fate-droid/coursera.git"
  },
  "author": "Fabio Bareiro Fleitas",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Fate-droid/coursera/issues"
  },
  "homepage": "https://github.com/Fate-droid/coursera#readme",
  "devDependencies": {
    "copyfiles": "^2.4.1",
    "cssmin": "^0.4.3",
    "grunt": "^1.6.1",
    "grunt-contrib-concat": "^2.1.0",
    "grunt-contrib-sass": "^2.0.0",
    "grunt-contrib-watch": "^1.1.0",
    "htmlmin": "^0.0.7",
    "lite-server": "^2.6.1",
    "onchange": "^7.1.0",
    "rimraf": "^6.0.1",
    "ruby": "^0.6.1",
    "sass": "^1.81.0",
    "uglifyjs": "^2.4.11",
    "usemin-cli": "^0.0.7"
  },
  "dependencies": {
    "bootstrap": "^5.3.3",
    "concurrently": "^9.1.0",
    "imagemin": "^9.0.0",
    "jquery": "^3.7.1",
    "open-iconic": "^1.1.1",
    "popper.js": "^1.16.1"
  },
  "keywords": []
}
